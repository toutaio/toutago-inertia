{
  "name": "@toutaio/inertia-vue",
  "version": "0.1.0",
  "description": "Vue adapter for Toutago Inertia.js implementation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "inertia",
    "vue",
    "toutago",
    "ssr",
    "spa"
  ],
  "author": "Toutaio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toutaio/toutago-inertia.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/toutaio/toutago-inertia/issues"
  },
  "homepage": "https://github.com/toutaio/toutago-inertia/tree/main/packages/vue#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.54.0",
    "happy-dom": "^12.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0"
  }
}
