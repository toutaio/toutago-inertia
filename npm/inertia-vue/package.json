{
  "name": "@toutaio/inertia-vue",
  "version": "0.1.0",
  "description": "Inertia.js Vue 3 adapter for Toutā Go framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "inertia",
    "vue",
    "toutago",
    "spa",
    "ssr"
  ],
  "author": "Toutā Team",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/test-utils": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "vue": "^3.4.5",
    "@vitest/coverage-v8": "^1.1.3"
  },
  "dependencies": {
    "axios": "^1.6.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toutaio/toutago-inertia.git",
    "directory": "npm/inertia-vue"
  },
  "bugs": {
    "url": "https://github.com/toutaio/toutago-inertia/issues"
  },
  "homepage": "https://github.com/toutaio/toutago-inertia/tree/main/npm/inertia-vue#readme"
}
